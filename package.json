{
  "name": "awssecplat-webextension",
  "displayName": "awssecplat-webextension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "awssecplat-webextension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "dockerContainer.showDockerFileTypeView",
        "title": "Show Docker File Type View"
      },
      {
        "command": "dockerContainer.selectContainer",
        "title": "Select Mode"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "awssecplat",
          "title": "Awareness Security Platform",
          "icon": "media/container-security.png"
        }
      ]
    },
    "views": {
      "awssecplat": [
        {
          "id": "dockerContainer",
          "name": "Docker File Type Selection"
        },
        {
          "id": "dockerScriptView",
          "name": "Docker Script Manager"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "dockerContainer.selectContainer",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/vscode": "^1.54.0",
    "@types/vscode-webview": "^1.57.4",
    "@types/webpack-env": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-web": "^0.0.48",
    "assert": "^2.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.52.0",
    "mocha": "^10.2.0",
    "process": "^0.11.10",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.3.1"
  }
}
